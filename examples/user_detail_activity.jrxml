	<subDataset name="total user activity">
		<parameter name="username" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
	t.reportname,
	count(o.id),
	o.username
from
	ar_report_order o,
	ar_report_template t
where
	t.id = o.report_id and
	o.username = $P{username}
group by
	t.reportname,
	o.username
order by
	count desc]]>
		</queryString>
		<field name="reportname" class="java.lang.String"/>
		<field name="count" class="java.lang.Long"/>
		<field name="username" class="java.lang.String"/>
	</subDataset>
	<subDataset name="preferred format">
		<parameter name="username" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select output_format as format, count(output_format)
from arl_report_order
where username = $P{username}
group by output_format]]>
		</queryString>
		<field name="format" class="java.lang.String"/>
		<field name="count" class="java.lang.Long"/>
	</subDataset>
	<parameter name="datasource" class="java.lang.String" isForPrompting="false">
		<parameterDescription><![CDATA[java:comp/env/jdbc/aperte-reports]]></parameterDescription>
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="username" class="java.lang.String">
		<property name="input_type" value="select"/>
		<property name="dict_query" value="java:comp/env/jdbc/aperte-reports; select distinct(username), username as displayed_username from ar_report_order"/>
		<property name="label" value="user name selection"/>
		<property name="required" value="true"/>
		<property name="required_error" value="user name is required!"/>
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="email_flag" class="java.lang.String">
		<property name="input_type" value="checkbox"/>
		<property name="label" value="show only reports sent to email"/>
		<defaultValueExpression><![CDATA[0]]></defaultValueExpression>
	</parameter>
	<parameter name="date_from" class="java.lang.String">
		<property name="input_type" value="date"/>
		<property name="label" value="input start date"/>
	</parameter>
	<parameter name="date_to" class="java.lang.String">
		<property name="input_type" value="date"/>
		<property name="label" value="input end date"/>
	</parameter>
	<queryString>
		<![CDATA[select
        t.reportname,
        count(o.id),
        to_char(date_trunc('day', o.start_date), 'YY.MM.DD') as day
from
        ar_report_order o,
        ar_report_template t
where
        t.id = o.report_id
        and ($P{date_from} = null or $P{date_from} = '' or o.start_date >= to_date($P{date_from}, 'DD-MM-YYYY HH24:MI'))
        and ($P{date_to} = null or $P{date_to} = '' or o.start_date <= to_date($P{date_to}, 'DD-MM-YYYY HH24:MI'))
        and o.username = $P{username}
        and (o.recipient_email is null = ($P{email_flag} = 'false') or o.recipient_email is not null)
group by
        t.reportname,
        date_trunc('day', o.start_date)]]>
	</queryString>
	<field name="reportname" class="java.lang.String"/>
	<field name="count" class="java.lang.Long"/>
	<field name="day" class="java.lang.String"/>
