	<subDataset name="weekly by day">
		<queryString>
			<![CDATA[select
        t.reportname,
        count(o.id),
        to_char(o.start_date, 'day') as day
from
        ar_report_order o,
        ar_report_template t
where
        t.id = o.report_id and
        o.start_date >= now() - justify_days(interval '7 days')
group by
        t.reportname,
        to_char(o.start_date, 'day')]]>
		</queryString>
		<field name="reportname" class="java.lang.String"/>
		<field name="count" class="java.lang.Long"/>
		<field name="day" class="java.lang.String"/>
	</subDataset>
